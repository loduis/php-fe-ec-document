{
    "name": "loduis/feec-document",
    "authors": [
        {
            "name": "Loduis Madariaga",
            "email": "loduis@gmail.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "FEEC\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "FEEC\\Tests\\": "tests/"
        }
    },
    "require": {
        "php": "^7.4|^8.0",
        "loduis/xml-document": "master",
        "loduis/feec-helpers": "dev-main",
        "picqer/php-barcode-generator": "^2.4"
    },
    "require-dev": {
        "loduis/xml-tests": "master"
    },
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/loduis/php-fe-ec-helpers.git"
        },
        {
            "type": "vcs",
            "url": "https://github.com/loduis/php-support.git"
        }
    ],
    "prefer-stable": true,
    "prefer-dist": true,
    "minimum-stability": "dev",
    "config": {
        "bin-dir": "bin"
    }
}
